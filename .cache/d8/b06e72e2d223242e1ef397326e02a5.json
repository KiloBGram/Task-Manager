button{\r\n    height: 3em;\r\n    width: 5em;\r\n    border: 2px solid $main-color;\r\n    border-radius: 3px;\r\n    background-color: $main-color;\r\n    color:white;\r\n    padding:6px;\r\n    transition: 100ms;\r\n}\r\n.manager_create-task-button:hover{\r\n    background-color: darken($main-color, 15%);\r\n    border-color: darken($main-color, 15%);\r\n}\r\n.manager_horizontal-line{\r\n    border: 1px solid $light-color;\r\n    width: 80vw;\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n}\r\n.task{\r\n    width: 100%;\r\n    height: 50px;\r\n    display: grid;\r\n    grid-template-columns: 25fr 1fr 1fr 2fr 1fr;\r\n    grid-template-areas: 'main checkbox order edit delete';\r\n    font-size:1.5em;\r\n    box-shadow: \r\n    2px 0 0 0 $light-color, \r\n    0 2px 0 0 $light-color, \r\n    2px 2px 0 0 $light-color,\r\n    2px 0 0 0 $light-color inset, \r\n    0 2px 0 0 $light-color inset;\r\n    border-radius: 3px;\r\n}\r\n.task_text-container{\r\n    margin-top: auto;\r\n    margin-bottom: auto;\r\n    padding-left: 15px;\r\n    grid-area: main;\r\n    border-right: 1px solid $light-color;\r\n    overflow-x: auto;\r\n    white-space: nowrap;\r\n}\r\n.task_container-3{\r\n    grid-area: delete;\r\n    height: 100%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n.task_delete-button{\r\n    margin: auto;\r\n    background-color: $warning-red;\r\n    color: white;\r\n    border-radius: 3px;\r\n    border: 2px solid $warning-red;\r\n    transition: 100ms;\r\n    width: 2em;\r\n    height: 2em;\r\n}\r\n.task_delete-button:hover{\r\n    background-color: darken($warning-red, 15%);\r\n    border-color: darken($warning-red, 15%);\r\n}\r\n.task_container-1{\r\n    grid-area: checkbox;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n.task_checkbox{\r\n    width: 2em;\r\n    height: 2em;\r\n}\r\n.task_container-2{\r\n    grid-area: edit;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n.task_edit-button{\r\n    width: 4em;\r\n    height: 2em;\r\n    background-color: $main-color;\r\n    border: 2px solid $main-color;\r\n    border-radius: 3px;\r\n    transition: 100ms;\r\n    color: white;\r\n}\r\n.task_edit-button:hover{\r\n    background-color: darken($main-color, 15%);\r\n    border-color: darken($main-color, 15%);\r\n}\r\nbutton:focus, input:focus{\r\n    outline: none;\r\n}\r\n.task_save-button{\r\n    width: 4em;\r\n    height: 2em;\r\n    background-color: $accept-green;\r\n    border: 2px solid $accept-green;\r\n    border-radius: 3px;\r\n    transition: 100ms;\r\n    color: white;\r\n    display: none;\r\n}\r\n.task_save-button:hover{\r\n    background-color: darken($accept-green, 15%);\r\n    border-color: darken($accept-green, 15%);\r\n}\r\n.task_text-edit-input{\r\n    width: 12vw;\r\n    height: 2em;\r\n    border: 2px solid $main-color;\r\n    border-radius: 3px;\r\n    padding:5px;\r\n}\r\n.task_container-order{\r\n    grid-area: order;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n.order_button{\r\n    width:2em;\r\n    height:2em;\r\n    text-align: center;\r\n    color: $main-color;\r\n    border:2px solid $main-color;\r\n    background: white;\r\n    border-radius: 3px;\r\n    transition:100ms;\r\n}\r\n.order-left{\r\n    border-right: none;\r\n    border-top-right-radius: 0;\r\n    border-bottom-right-radius: 0;\r\n}\r\n.order-right{\r\n    border-left: none;\r\n    border-top-left-radius: 0;\r\n    border-bottom-left-radius: 0;\r\n}\r\n.order_button:hover{\r\n    color: white;\r\n    background-color: $main-color;\r\n}"},"lineCount":null}},"error":null,"hash":"d70deaa9afbc3b4bf4b0bacae15ffe7e","cacheData":{"env":{}}}